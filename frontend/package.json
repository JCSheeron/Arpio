{
  "name": "arpio-frontend",
  "version": "0.1.0",
  "description": "Raspberry Pi I/O Control",
  "main": "index.js",
  "scripts": {
    "install-frontend": "npm install",
    "clean": "rimraf dist",
    "start": "webpack-dev-server --config webpack-frontend.config.js",
    "build": "webpack --config webpack-frontend.config.js -p",
    "build-dev": "webpack --config webpack-frontend.config.js --debug --devtool inline-source-map --output-pathinfo",
    "build-watch": "webpack --config webpack-frontend.config.js --watch --debug --devtool cheap-module-eval-source-map --output-pathinfo",
    "build-clean-watch": "npm run clean && npm run build-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCSheeron/Arpio.git"
  },
  "keywords": [
    "express",
    "handlebars",
    "javascript",
    "material-ui",
    "react-router",
    "mondodb",
    "node",
    "raspberry pi"
  ],
  "author": "JCSheeron",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JCSheeron/Arpio/issues"
  },
  "homepage": "https://github.com/JCSheeron/Arpio#readme",
  "dependencies": {
    "@date-io/date-fns": "^2.10.6",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@mdi/js": "^5.5.55",
    "@mdi/react": "^1.4.0",
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "react-table": "^7.2.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^7.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
