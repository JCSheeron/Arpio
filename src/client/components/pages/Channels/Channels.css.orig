/*  Global Styles
---------------------------------------- */
@font-face {
  font-family: 'RobotoLight';
  src: url(fonts/Roboto/Roboto-Light.ttf);
}
@font-face {
  font-family: 'RobotoRegular';
  src: url(fonts/Roboto/Roboto-Regular.ttf);
}
@font-face {
  font-family: 'RobotoBold';
  src: url(fonts/Roboto/Roboto-Bold.ttf);
}

footer {
  font-family: 'RobotoBold', sans-serif;
  font-weight: 700;
  margin: 0;
}

h1,
h2 {
  font-family: 'RobotoBold', sans-serif;
  font-weight: 400;
  margin: 0;
}

h1 {
  font-size: 100px;
  line-height: 1;
}

h2 {
  font-size: 45px;
}

/* channel items
---------------------------------------- */
.channelItem {
  background-color: gray;
  color: red;
  width: 15;
  height: 15;
}

/*  Profile
---------------------------------------- */
header {
  background: #343434; /* dark gray, almost black */
  color: #f0efeb; /* isabelline */
}

/*  Projects
---------------------------------------- */
.projects {
  background: #fff1e6; /* linen */
}
.projects a {
}

.projects .btn {
  color: #f7fff7;
  background: #2f3061;
  text-decoration: none;
  padding: 8px;
  border-radius: 4px;
  display: inline-block;
}

.projects .btn:hover {
  background: #2f3061cd;
}

/*  Work Experience
---------------------------------------- */
.workExperience {
  background: #a5a58d; /* artichoke */
}

